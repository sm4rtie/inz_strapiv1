{
  "routes": {
    "GET /order": {
      "controller": "Order",
      "action": "find",
      "policies": [
        "isAuthorized"
      ]
    },
    "GET /order/:id": {
      "controller": "Order",
      "action": "findOne",
      "policies": [
        "isAuthorized"
      ]
    },
    "POST /order": {
      "controller": "Order",
      "action": "create",
      "policies": [
        "isAuthorized",
        "addDataCreate"
      ]
    },
    "PUT /order/:id": {
      "controller": "Order",
      "action": "update",
      "policies": [
        "isAuthorized",
        "addDataUpdate"
      ]
    },
    "DELETE /order/:id": {
      "controller": "Order",
      "action": "destroy",
      "policies": [
        "isAuthorized"
      ]
    },
    "POST /order/:parentId/:relation": {
      "controller": "Order",
      "action": "add",
      "policies": [
        "isAuthorized",
        "addDataCreate"
      ]
    },
    "DELETE /order/:parentId/:relation/:id": {
      "controller": "Order",
      "action": "remove",
      "policies": [
        "isAuthorized",
        "addDataUpdate"
      ]
    }
  }
